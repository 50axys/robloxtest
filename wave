local function RRPPXO_fake_script() -- ImageButton.LocalScript 
	local script = Instance.new('LocalScript', ImageButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Main.Visible = not script.Parent.Parent.Main.Visible
		script.Parent.Parent.Main.Size = UDim2.new(0.331, 0,0.351, 0)
	end)
end
coroutine.wrap(RRPPXO_fake_script)()

